{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/vote",
      "dest": "/api/vote.js"
    },
    {
      "src": "/api/votes",
      "dest": "/api/votes.js"
    },
    {
      "src": "/api/stats",
      "dest": "/api/stats.js"
    }
  ]
}